        // FAQ Accordion
        document.querySelectorAll('.faq-question').forEach(question => {
            question.addEventListener('click', () => {
                const item = question.parentElement;
                item.classList.toggle('active');
            });
        });

        // Email Permutator Logic
        document.addEventListener('DOMContentLoaded', function() {
            const firstName = document.getElementById('firstName');
            const lastName = document.getElementById('lastName');
            const domain = document.getElementById('domain');
            const generateBtn = document.getElementById('generateBtn');
            const resultsArea = document.getElementById('resultsArea');
            const emailList = document.getElementById('emailList');
            const resultsCount = document.getElementById('resultsCount');
            const copyAllBtn = document.getElementById('copyAllBtn');
            const gmailBtn = document.getElementById('gmailBtn');
            
            // Generate Email Permutations
            generateBtn.addEventListener('click', function() {
                const first = firstName.value.trim().toLowerCase();
                const last = lastName.value.trim().toLowerCase();
                const domainValue = domain.value.trim().toLowerCase();
                
                if (!first || !last || !domainValue) {
                    alert('Please fill in all fields to generate email permutations.');
                    return;
                }
                
                // Clean domain (remove http://, https://, www. if present)
                let cleanDomain = domainValue.replace(/^https?:\/\//, '').replace(/^www\./, '');
                
                // Generate email permutations
                const permutations = generateEmailPermutations(first, last, cleanDomain);
                
                // Display results
                displayResults(permutations);
            });
            
            // Generate all possible email permutations
            function generateEmailPermutations(first, last, domain) {
                const firstInitial = first.charAt(0);
                const lastInitial = last.charAt(0);
                
                const templates = [
                    // First name variations
                    `${first}@${domain}`,
                    `${firstInitial}@${domain}`,
                    
                    // Last name variations
                    `${last}@${domain}`,
                    `${lastInitial}@${domain}`,
                    
                    // Combined with dots
                    `${first}.${last}@${domain}`,
                    `${firstInitial}.${last}@${domain}`,
                    `${first}.${lastInitial}@${domain}`,
                    `${firstInitial}.${lastInitial}@${domain}`,
                    
                    // Combined with underscores
                    `${first}_${last}@${domain}`,
                    `${firstInitial}_${last}@${domain}`,
                    `${first}_${lastInitial}@${domain}`,
                    `${firstInitial}_${lastInitial}@${domain}`,
                    
                    // Combined with hyphens
                    `${first}-${last}@${domain}`,
                    `${firstInitial}-${last}@${domain}`,
                    `${first}-${lastInitial}@${domain}`,
                    `${firstInitial}-${lastInitial}@${domain}`,
                    
                    // Combined without separator
                    `${first}${last}@${domain}`,
                    `${firstInitial}${last}@${domain}`,
                    `${first}${lastInitial}@${domain}`,
                    `${firstInitial}${lastInitial}@${domain}`,
                    
                    // Last name first
                    `${last}.${first}@${domain}`,
                    `${last}_${first}@${domain}`,
                    `${last}-${first}@${domain}`,
                    `${last}${first}@${domain}`,
                    
                    // Last name + first initial
                    `${last}.${firstInitial}@${domain}`,
                    `${last}_${firstInitial}@${domain}`,
                    `${last}-${firstInitial}@${domain}`,
                    `${last}${firstInitial}@${domain}`,
                    
                    // Additional variations
                    `${first}${last[0]}${last[last.length-1]}@${domain}`,
                    `${first}.${last[0]}${last[last.length-1]}@${domain}`,
                    `${first[0]}${first[first.length-1]}${last}@${domain}`,
                    `${first[0]}${first[first.length-1]}.${last}@${domain}`,
                    
                    // Company specific formats
                    `${first}${last[0]}@${domain}`,
                    `${first[0]}${last}@${domain}`
                ];
                
                return templates;
            }
            
            // Display the generated email permutations
            function displayResults(permutations) {
                // Clear previous results
                emailList.innerHTML = '';
                
                // Update count
                resultsCount.textContent = `${permutations.length} emails generated`;
                
                // Create email items
                permutations.forEach(email => {
                    const emailItem = document.createElement('div');
                    emailItem.className = 'email-item';
                    
                    const emailText = document.createElement('div');
                    emailText.textContent = email;
                    
                    const copyButton = document.createElement('button');
                    copyButton.className = 'email-copy tooltip';
                    copyButton.innerHTML = '<i class="far fa-copy"></i><span class="tooltiptext">Copy</span>';
                    copyButton.onclick = function() {
                        copyToClipboard(email);
                        this.querySelector('.tooltiptext').textContent = 'Copied!';
                        setTimeout(() => {
                            this.querySelector('.tooltiptext').textContent = 'Copy';
                        }, 2000);
                    };
                    
                    emailItem.appendChild(emailText);
                    emailItem.appendChild(copyButton);
                    emailList.appendChild(emailItem);
                });
                
                // Show results area
                resultsArea.style.display = 'block';
                
                // Scroll to results
                resultsArea.scrollIntoView({ behavior: 'smooth' });
            }
            
            // Copy all emails to clipboard
            copyAllBtn.addEventListener('click', function() {
                const allEmails = Array.from(emailList.querySelectorAll('.email-item'))
                    .map(item => item.firstChild.textContent)
                    .join('\n');
                
                copyToClipboard(allEmails);
                
                // Change button text temporarily
                const originalText = this.querySelector('span').textContent;
                this.querySelector('span').textContent = 'Copied!';
                
                setTimeout(() => {
                    this.querySelector('span').textContent = originalText;
                }, 2000);
            });
            
            // Open in Gmail
            gmailBtn.addEventListener('click', function() {
                const allEmails = Array.from(emailList.querySelectorAll('.email-item'))
                    .map(item => item.firstChild.textContent)
                    .join(',');
                
                window.open(`https://mail.google.com/mail/?view=cm&fs=1&to=${encodeURIComponent(allEmails)}`, '_blank');
            });
            
            // Helper function to copy text to clipboard
            function copyToClipboard(text) {
                const textarea = document.createElement('textarea');
                textarea.value = text;
                document.body.appendChild(textarea);
                textarea.select();
                document.execCommand('copy');
                document.body.removeChild(textarea);
            }
            
            // Satisfaction Rating
            document.querySelectorAll('.rating-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    // Reset all buttons
                    document.querySelectorAll('.rating-btn').forEach(b => {
                        b.style.transform = 'scale(1)';
                    });
                    
                    // Highlight selected button
                    this.style.transform = 'scale(1.2)';
                    
                    // Here you would typically send the rating to your server
                    // For this example, we'll just show a thank you message
                    setTimeout(() => {
                        const parent = this.parentElement.parentElement;
                        parent.innerHTML = '<p>Thank you for your feedback!</p>';
                    }, 500);
                });
            });
        });
    </script></body></html>
