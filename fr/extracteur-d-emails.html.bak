        function extractEmailsFromText(text) {
            if (!text) return { emails: [], count: 0 };
            const clean = text.replace(/<[^>]*>/g,' ');
            const words = clean.split(/\s+/);
            const candidates = words.filter(w=> w.includes('@'));
            const cleanList = candidates.map(e=> e.replace(/^[.,;:()"'<>\[\]{}]+|[.,;:!?)"'<>\[\]{}]+$/g,''));
            const unique = [...new Set(cleanList)];
            return { emails: unique, count: unique.length };
        }
        document.getElementById('extractBtn').addEventListener('click', function(){
            const txt = document.getElementById('emailsSource').value;
            const btn = this.querySelector('span'); const loader = this.querySelector('.loader');
            btn.textContent='Extraction en cours...'; loader.style.display='inline-block';
            setTimeout(()=>{
                const res = extractEmailsFromText(txt);
                document.getElementById('emailsResult').value = res.emails.join('\n');
                document.getElementById('resultArea').style.display='block';
                btn.textContent='Extraire les emails'; loader.style.display='none';
            },500);
        });
        document.getElementById('copyBtn').addEventListener('click', function(){
            const out = document.getElementById('emailsResult'); out.select(); document.execCommand('copy'); this.textContent='Copié !'; setTimeout(()=>this.textContent='Copier',2000);
        });
        document.getElementById('downloadBtn').addEventListener('click', function(){
            const data = document.getElementById('emailsResult').value;
            const blob = new Blob([data],{type:'text/plain'});
            const url = URL.createObjectURL(blob);
            const a=document.createElement('a'); a.href=url; a.download='emails.txt'; document.body.appendChild(a); a.click(); document.body.removeChild(a);
        });
